{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": "src",
    "module": "es6",
    "declaration": true,
    "target": "es6",
    "sourceMap": false,
    "outDir": "dist/esm",
    "lib": ["ESNext"],
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "strict": true
  },
  "include": [
    "src/**/*.ts"
  ]
}
